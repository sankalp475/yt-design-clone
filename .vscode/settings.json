{
	"workbench.editorAssociations": {
        "*.woff2": "default"
    },
	"editor.fontFamily": "'Cartograph CF', Operater Mono",
	"editor.fontSize": 22,
	"editor.lineHeight": 48,
	"editor.tokenColorCustomizations": {
		"textMateRules": [{
			"scope": [
				"comments",
				"entity.attribute.name",
				"entity.others.attribute-name",
				"entity.others.attribute-name.class",
				"entity.others.attribute-name.id",
				"keyword",
				"constant",
				"storage.modifier",
				"storage.type",
				"storage.type.function",
				"storage.type.class",
				"variable.parameter",
				"support.type.property-name.css",
				"source.css",
				"entity.name.tag.html",
				"meta.tag.structure.body.start.html",
				"text.html.derivative"
			],
			"settings": {
				"fontStyle": "italic"
			}
		}]
	},
	"editor.guides.bracketPairs": "active",
	"editor.guides.bracketPairsHorizontal": "active",
	"editor.bracketPairColorization.enabled": true
}



